{
    "name": "LightProvider",
    "version": "1.0.0",
    "main": "main.js",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "electron": "electron .",
        "ng-electron-build": "rimraf dist & ng build --base-href ./",
        "package": "electron-packager .",
        "package:win": "electron-packager . LightProvider --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico  --extra-resource=../dist --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='ELightProvider'",
        "package:mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds --extra-resource=../dist",
        "package:linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --extra-resource=../dist --prune=true --out=release-builds"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~9.1.7",
        "@angular/common": "~9.1.7",
        "@angular/compiler": "~9.1.7",
        "@angular/core": "~9.1.7",
        "@angular/forms": "~9.1.7",
        "@angular/platform-browser": "~9.1.7",
        "@angular/platform-browser-dynamic": "~9.1.7",
        "@angular/router": "~9.1.7",
        "@types/cuint": "^0.2.1",
        "cross-zip": "^4.0.0",
        "cuint": "^0.2.2",
        "file-system": "^1.2.2",
        "node": "^14.4.0",
        "rxjs": "~6.5.4",
        "sanitize-filename": "^1.6.3",
        "tmp-promise": "^3.0.2",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.6",
        "@angular/cli": "~9.1.6",
        "@angular/compiler-cli": "~9.1.7",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.20.7",
        "codelyzer": "^5.1.2",
        "electron": "^9.4.4",
        "electron-packager": "^15.2.0",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.0",
        "karma-jasmine": "~3.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "~5.4.3",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.8.3"
    }
}